#: ../terminal/main.c:42
msgid "Usage: Terminal [OPTION...]"
msgstr "שימוש: Terminal [option...]"

#: ../terminal/main.c:44
msgid "  -h, --help                          Print this help message and exit"
msgstr "  -h, --help                        הדפס הודעת עזרה זו וצא"

#: ../terminal/main.c:45
msgid ""
"  -v, --version                       Print version information and exit"
msgstr "  -v, --version                          מדפיס מידע לגבי הגרסה ויוצא"

#: ../terminal/main.c:46
msgid ""
"  --disable-server                    Do not register with the D-BUS\n"
"                                      session message bus"
msgstr ""
"..--disable-server                       אל תרשם באפיק ההודעות \n"
"של הפעלת D-BUS"

#: ../terminal/main.c:49
msgid ""
"  -x, --execute                       Execute the remainder of the command\n"
"                                      line inside the terminal"
msgstr ""
"  -x, --execute                        הרץ את שארית שורת הפקודה בתוך המסוף."

#: ../terminal/main.c:51
msgid ""
"  -e, --command=STRING                Execute the argument to this option\n"
"                                      inside the terminal"
msgstr ""
"  -e, --comand=STRING                     הרץ את ארגומנט אפשרות זו במסוף."

#: ../terminal/main.c:53
msgid ""
"  --working-directory=DIRNAME         Set the terminals working directory"
msgstr "  --workin-directory=DIRNAME           מגדיר את ספרית העבודה של המסוף."

#: ../terminal/main.c:54
msgid "  -T, --title=TITLE                   Set the terminals title"
msgstr "  -T, --title=TITLE                  מגדיר את כותרת המסוף."

#: ../terminal/main.c:55
#, fuzzy
msgid ""
"  -H, --hold                          Do not immediatly destroy the tab\n"
"                                      when the child command exits"
msgstr ""
"  -x, --execute                        הרץ את שארית שורת הפקודה בתוך המסוף."

#: ../terminal/main.c:58
msgid ""
"  --display=DISPLAY                   X display to use for the last-\n"
"                                      specified window"
msgstr ""
"  --display=DISPLAY                       צג ה- X בו יש להשתמש עבור החלון "
"האחרון שצוין."

#: ../terminal/main.c:60
msgid ""
"  --geometry=GEOMETRY                 X geometry specification (see \"X\"\n"
"                                      man page), can be specified once per\n"
"                                      window to be opened"
msgstr ""
"  --geometry=GEOMETRY                   הגדרות מיקום וגודל ה-X (עוד בדף ה- "
"man של\"X\")\n"
"ניתן לציון פעם בודדת עבור כל חלון שיפתח."

#: ../terminal/main.c:63
msgid ""
"  --role=ROLE                         Set the role for the last-specified;\n"
"                                      window; applies to only one window;\n"
"                                      can be specified once for each window\n"
"                                      you create from the command line"
msgstr ""
"  --role=ROLE                      מגדיר את התפקיד עבור החלון האחרון שצוין\n"
"מיושם לחלון אחד בלבד. ניתן לציון פעם בודדת\n"
"לכל חלון שנוצר משורת הפקודה."

#: ../terminal/main.c:67
msgid ""
"  --startup-id=STRING                 ID for the startup notification\n"
"                                      protocol"
msgstr ""
"  --startup-id-STRING                 מספר זיהוי (ID) עבור פרוטוקול הודעות "
"האתחול."

#: ../terminal/main.c:69
msgid ""
"  --fullscreen                        Set the last-specified window into\n"
"                                      fullscreen mode; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line."
msgstr ""
"  --fullscreen                         מגדיר את החלון האחרון שצוין למצב מסך-"
"מלא\n"
"                                  מיושם לחלון אחד בלבד. ניתן לציון פעם "
"בודדת\n"
"                                  לכל חלון שנוצר משורת הפקודה."

#: ../terminal/main.c:74
msgid ""
"  --show-menubar                      Turn on the menubar for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --show-menubar                      מדליק את סרגל התפריטים לחלון האחרון "
"שצוין\n"
"                                  מיושם לחלון אחד בלבד. ניתן לציון פעם "
"בודדת\n"
"                                  לכל חלון שנוצר משורת הפקודה."

#: ../terminal/main.c:79
msgid ""
"  --hide-menubar                      Turn off the menubar for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --hide-menubar                    סגור את סרגל התפריטים לחלון האחרון "
"שצוין\n"
"                                  מיושם לחלון אחד בלבד. ניתן לציון פעם "
"בודדת\n"
"                                  לכל חלון שנוצר משורת הפקודה."

#: ../terminal/main.c:84
msgid ""
"  --show-borders                      Turn on the window decorations for\n"
"                                      the last-specified window; applies\n"
"                                      to only one window; can be specified\n"
"                                      once for each window you create from\n"
"                                      the command line"
msgstr ""
"  --show-borders                    הפעל את עיטורי החלון עבור החלון האחרון "
"שצוין\n"
"                                           מיושם על חלון אחד בלבד. ניתן "
"לציון פעם בודדת\n"
"                                           לכל חלון שנוצר משורת הפקודה."

#: ../terminal/main.c:89
msgid ""
"  --hide-borders                      Turn off the window decorations for\n"
"                                      the last-specified window; applies\n"
"                                      to only one window; can be specified\n"
"                                      once for each window you create from\n"
"                                      the command line"
msgstr ""
"  --hide-borders                    סגור את עיטורי החלון עבור החלון האחרון "
"שצוין\n"
"                                           מיושם על חלון אחד בלבד. ניתן "
"לציון פעם בודדת\n"
"                                           לכל חלון שנוצר משורת הפקודה."

#: ../terminal/main.c:94
msgid ""
"  --show-toolbars                     Turn on the toolbars for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --show-toolbar                    הפעל את סרגלי הכלים לחלון האחרון שצוין\n"
"                                  מיושם לחלון אחד בלבד. ניתן לציון פעם "
"בודדת\n"
"                                  לכל חלון שנוצר משורת הפקודה."

#: ../terminal/main.c:99
msgid ""
"  --hide-toolbars                     Turn off the toolbars for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --hide-toolbar                    הסגור את סרגלי הכלים לחלון האחרון שצוין\n"
"                                  מיושם לחלון אחד בלבד. ניתן לציון פעם "
"בודדת\n"
"                                  לכל חלון שנוצר משורת הפקודה."

#: ../terminal/main.c:105
msgid ""
"  --tab                               Open a new tab in the last-specified\n"
"                                      window; more than one of these\n"
"                                      options can be provided"
msgstr ""
"  --tab                                פתח כרטיסיה חדשה בחלון האחרון שצוין\n"
"                                         ניתן להזין יותר מאפשרות אחת שכזו."

#: ../terminal/main.c:108
msgid ""
"  --window                            Open a new window containing one tab;\n"
"                                      more than one of these options can be\n"
"                                      provided"
msgstr ""
"  --window                             פתח חלון חדש המכיל כרטיסיה אחת\n"
"                                           ניתן להזין יותר מאפשרות אחת שכזו"

#: ../terminal/main.c:112
msgid "  --default-display=DISPLAY           default X display to use"
msgstr "  --default-display=DISPLAY           תצוגת ברירת המחדל לשימוש של X."

#: ../terminal/main.c:113
msgid ""
"  --default-working-directory=DIRNAME Set the default terminals working\n"
"                                      directory"
msgstr ""
"  --default-working-directory=DIRNAME   מגדיר את ספרית העבודה של מסוף ברירת "
"המחדל."

