#: terminal/main.c:228
msgid "Usage: Terminal [OPTION...]"
msgstr "Käyttö: Terminal [VALITSIN...]"

#: terminal/main.c:230
msgid "  -h, --help                          Print this help message and exit"
msgstr "  -h, --help                          Tulosta tämä ohje ja poistu"

#: terminal/main.c:231
msgid ""
"  -v, --version                       Print version information and exit"
msgstr "  -v, --version                       Tulosta versiotiedot ja poistu"

#: terminal/main.c:232
msgid ""
"  --disable-server                    Do not register with the D-BUS\n"
"                                      session message bus"
msgstr ""
"  --disable-server                    Älä rekisteröidy D-BUS-viestiväylän\n"
"                                      yhteyteen"

#: terminal/main.c:235
msgid ""
"  -x, --execute                       Execute the remainder of the command\n"
"                                      line inside the terminal"
msgstr ""
"  -x, --execute                       Suorita komentorivin loppuosa\n"
"                                      päätteessä"

#: terminal/main.c:237
msgid ""
"  -e, --command=STRING                Execute the argument to this option\n"
"                                      inside the terminal"
msgstr ""
"  -e, --command=MERKKIJONO            Suorita tämän valitsimen argumentti\n"
"                                      päätteessä"

#: terminal/main.c:239
msgid ""
"  --working-directory=DIRNAME         Set the terminals working directory"
msgstr "  --working-directory=HAKEMISTO       Aseta päätteen työhakemisto"

#: terminal/main.c:240
msgid "  -T, --title=TITLE                   Set the terminals title"
msgstr "  -T, --title=OTSAKE                  Aseta pääteikkunan otsake"

#: terminal/main.c:242
msgid ""
"  --display=DISPLAY                   X display to use for the last-\n"
"                                      specified window"
msgstr ""
"  --display=NÄYTTÖ                    X:n näyttö, jota viimeksi valittu\n"
"                                      ikkuna käyttää"

#: terminal/main.c:244
msgid ""
"  --geometry=GEOMETRY                 X geometry specification (see \"X\"\n"
"                                      man page), can be specified once per\n"
"                                      window to be opened"
msgstr ""
"  --geometry=GEOMETRIA                X:n geometriamääritys (katso \"man X"
"\",\n"
"                                      voi määrittää kerran avattavaa\n"
"                                      ikkunaa kohden"

#: terminal/main.c:247
msgid ""
"  --role=ROLE                         Set the role for the last-specified;\n"
"                                      window; applies to only one window;\n"
"                                      can be specified once for each window\n"
"                                      you create from the command line"
msgstr ""
"  --role=ROOLI                        Aseta rooli viimeksi määritetylle\n"
"                                      ikkunalle; koskee vain yhtä ikkunaa;\n"
"                                      voi määrittää kerran jokaiselle\n"
"                                      komentoriviltä luodulle ikkunalle"

#: terminal/main.c:251
msgid ""
"  --startup-id=STRING                 ID for the startup notification\n"
"                                      protocol"
msgstr ""
"  --startup-id=MERKKIJONO             Tunniste käynnistyksen huomautus-\n"
"                                      protokollaa varten"

#: terminal/main.c:253
msgid ""
"  --fullscreen                        Set the last-specified window into\n"
"                                      fullscreen mode; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line."
msgstr ""
"  --fullscreen                        Aseta viimeksi määritetty ikkuna\n"
"                                      täysruututilaan; koskee ainoastaan\n"
"                                      yhtä ikkunaa; voi määrittää kerran\n"
"                                      jokaista komentoriviltä luotua\n"
"                                      ikkunaa kohden"

#: terminal/main.c:258
msgid ""
"  --show-menubar                      Turn on the menubar for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --show-menubar                      Asettaa valikkopalkin näkyviin "
"viimeksi\n"
"                                      määritetyssä ikkunassa; koskee\n"
"                                      ainoastaan yhtä ikkunaa; voi "
"määrittää\n"
"                                      kerran jokaista komentoriviltä luotua\n"
"                                      ikkunaa kohden"

#: terminal/main.c:263
msgid ""
"  --hide-menubar                      Turn off the menubar for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --hide-menubar                      Piilottaa valikkopalkin näkyvistä\n"
"                                      viimeksi määritetyssä ikkunassa; "
"koskee\n"
"                                      ainoastaan yhtä ikkunaa; voi "
"määrittää\n"
"                                      kerran jokaista komentoriviltä luotua\n"
"                                      ikkunaa kohden"

#: terminal/main.c:268
msgid ""
"  --show-borders                      Turn on the window decorations for\n"
"                                      the last-specified window; applies\n"
"                                      to only one window; can be specified\n"
"                                      once for each window you create from\n"
"                                      the command line"
msgstr ""
"  --show-borders                      Asettaa ikkunan reunukset näkyviin\n"
"                                      viimeksi määritetyssä ikkunassa; "
"koskee\n"
"                                      ainoastaan yhtä ikkunaa; voi "
"määrittää\n"
"                                      kerran jokaista komentoriviltä luotua\n"
"                                      ikkunaa kohden"

#: terminal/main.c:273
msgid ""
"  --hide-borders                      Turn off the window decorations for\n"
"                                      the last-specified window; applies\n"
"                                      to only one window; can be specified\n"
"                                      once for each window you create from\n"
"                                      the command line"
msgstr ""
"  --hide-borders                      Piilottaa ikkunan reunukset viimeksi\n"
"                                      määritetyssä ikkunassa; koskee\n"
"                                      ainoastaan yhtä ikkunaa; voi "
"määrittää\n"
"                                      kerran jokaista komentoriviltä luotua\n"
"                                      ikkunaa kohden"

#: terminal/main.c:278
msgid ""
"  --show-toolbars                     Turn on the toolbars for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --show-toolbars                     Asettaa työkalupalkin näkyviin\n"
"                                      viimeksi määritetyssä ikkunassa; "
"koskee\n"
"                                      ainoastaan yhtä ikkunaa; voi "
"määrittää\n"
"                                      kerran jokaista komentoriviltä luotua\n"
"                                      ikkunaa kohden"

#: terminal/main.c:283
msgid ""
"  --hide-toolbars                     Turn off the toolbars for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --hide-toolbars                     Piilottaa työkalupalkin näkyvistä\n"
"                                      viimeksi määritetyssä ikkunassa; "
"koskee\n"
"                                      ainoastaan yhtä ikkunaa; voi "
"määrittää\n"
"                                      kerran jokaista komentoriviltä luotua\n"
"                                      ikkunaa kohden"

#: terminal/main.c:289
msgid ""
"  --tab                               Open a new tab in the last-specified\n"
"                                      window; more than one of these\n"
"                                      options can be provided"
msgstr ""
"  --tab                               Avaa uusi välilehti viimeksi "
"määritetyssä\n"
"                                      ikkunassa. Näitä valitsimia voi olla\n"
"                                      enemmän kuin yksi"

#: terminal/main.c:292
msgid ""
"  --window                            Open a new window containing one tab;\n"
"                                      more than one of these options can be\n"
"                                      provided"
msgstr ""
"  --window                            Avaa uusi ikkuna, jossa yksi "
"välilehti\n"
"                                      Näitä valitsimia voi olla enemmän\n"
"                                      kuin yksi"

#: terminal/main.c:296
msgid "  --default-display=DISPLAY           default X display to use"
msgstr "  --default-display=NÄYTTÖ            oletuksena käytettävä X:n näyttö"

#: terminal/main.c:297
msgid ""
"  --default-working-directory=DIRNAME Set the default terminals working\n"
"                                      directory"
msgstr ""
"  --default-working-directory=HAKEM.  Aseta oletuspäätteen työhakemisto"

