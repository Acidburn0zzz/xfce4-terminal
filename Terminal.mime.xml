<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-terminal-color-palette">
    <sub-class-of type="text/plain" />
    <comment>Terminal color palette</comment>
    <comment xml:lang="de">Terminal Farb-Palette</comment>
    <magic priority="50">
      <match value="[Terminal color palette]" type="string" offset="0:32" />
    </magic>
    <glob pattern="*.tcpalette"/>
  </mime-type>
</mime-info>
